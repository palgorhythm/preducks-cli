{
  "name": "preducks-cli",
  "version": "1.0.0",
  "description": "cli version of preducks electron app",
  "main": "./build/index.js",
  "types": "./src/Interfaces.d.ts",
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "test": "npm i -g && create-preducks-app",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/palgorhythm/preduckscli.git"
  },
  "bin": {
    "create-preducks-app": "./build/index.js"
  },
  "keywords": [
    "cli",
    "react",
    "redux",
    "typescript"
  ],
  "author": "jacob richards, max gonzalez, will napier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/palgorhythm/preduckscli/issues"
  },
  "homepage": "https://github.com/palgorhythm/preduckscli#readme",
  "dependencies": {
    "fs": "0.0.1-security",
    "nodemon": "^1.19.1",
    "ora": "^3.4.0",
    "prettier": "^1.18.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.3.3",
    "@types/node": "^12.6.8",
    "@typescript-eslint/parser": "^1.12.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.1",
    "typescript": "^3.5.3"
  }
}
